<!doctype html>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>SPlanner Staff</title>
    <link rel="stylesheet" href="/static/css/main.css" />
	<noscript><link rel="stylesheet" href="/static/css/noscript.css" /></noscript>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/xlsx/0.16.2/xlsx.full.min.js"></script>
    <script src="https://www.gstatic.com/firebasejs/8.2.7/firebase.js"></script>

    <!-- TODO: Add SDKs for Firebase products that you want to use
    https://firebase.google.com/docs/web/setup#available-libraries -->
    <script src="https://www.gstatic.com/firebasejs/8.2.7/firebase-analytics.js"></script>
    <script src="https://apis.google.com/js/platform.js" async defer></script>
    <meta name="google-signin-client_id" content="871154578045-ri18i7noosjripjq3gqnmgqrtnpn1prp.apps.googleusercontent.com">
</head>
<body class="is-preload landing">
    <div id="page-wrapper">
        <!-- Header -->
        <Header id="header">
            <h1 id="logo"><a>SPlanner</a></h1>
            <nav id="nav">
                <ul>

                </ul>
            </nav>
        </Header>

        <!-- Main -->
        <section id="one" class="wrapper style1 special fade-up">
            <!-- head -->
            <div class="content">
                <header>
                    <h2>Add curriculum</h2>
                </header>
            </div>
            <form method="post" action="#">
                <div class="row gtr-uniform gtr-50">
                    <!-- Get Excel -->
                    <div class="col-3 col-12-medium"></div>
                    <div class="col-3 col-12-medium">
                        <input class="" type="file" id="inputexcel" accept=".xls,.xlsx" onchange="changeExcel()">
                    </div>
                    <div class="col-3 col-12-medium">
                        <input type="button" value="Confirm" class="button primary" id="convert">
                    </div>
                    <div class="col-3 col-12-medium"></div>

                    <!-- Faculty -->
                    <div class="col-3 col-12-medium"></div>
                    <div class="col-6 col-12-xsmall">
                        <select name="Select Faculty" id="faculty" onchange="showfaculty()">
                            <option value="">Select Faculty</option>
                            <option value="human">01 Faculty of Humanity</option>
                            <option value="edu">02 Faculty of Education</option>
                            <option value="fineart">03 Faculty of Fine Art</option>
                            <option value="soc">04 Faculty of Social Sciences</option>
                            <option value="sci">05 Faculty of Science</option>
                            <option value="eng">06 Faculty of Engineering</option>
                            <option value="med">07 Faculty of Medicine</option>
                            <option value="agri">08 Faculty of Agriculture</option>
                            <option value="dent">09 Faculty of Dentistry</option>
                            <option value="phama">10 Faculty of Pharmacy</option>
                            <option value="medtec">11 Faculty of Associated Medical Sciences</option>
                            <option value="nurse">12 Faculty of Nursing</option>
                            <option value="argo">13 Faculty of Agro-Industry</option>
                            <option value="vet">14 Faculty of Veterinary Medicine</option>
                            <option value="accba">15 Faculty of Business Administration</option>
                            <option value="econ">16 Faculty of Economics</option>
                            <option value="arch">17 Faculty of Architecture</option>
                            <option value="masscom">18 Faculty of Mass Communication</option>
                            <option value="polsci">19 Faculty of Political Science and Public Administration</option>
                            <option value="law">20 Faculty of Law</option>
                            <option value="camt">21 Faculty of Collage of Art, Media and Technology</option>
                        </select>
                    </div>
                    <div class="col-3 col-12-medium"></div>

                    <!-- Bachelor -->
                    <div class="col-3 col-12-medium"></div>
                    <div class="col-6 col-12-xsmall">
                        <select name="Select Course" id="bachelor">
                            <option value="">Select Bachelor</option>
                        </select>
                    </div>
                    <div class="col-3 col-12-medium"></div>
                    <!-- Input course -->
                    <div class="col-3 col-12-medium"></div>
                    <div class="col-6 col-12-xsmall"  id="div3">
                        <input type="text" value="" placeholder="Curriculum" id="cos">
                    </div>
                    <div class="col-3 col-12-medium"></div>

                    <!-- Input Send -->
                    <div class="col-3 col-12-medium"></div>
                    <div class="col-6 col-12-xsmall"  id="div4">
                        <input type="button" value="Send" class="button primary" id="send">
                    </div>
                    <div class="col-3 col-12-medium"></div>
                </div>
                <div class="row gtr-uniform gtr-50">
                    <div class="col-9 col-12-medium"></div>
                    <div class="col-3 col-12-medium"><a href="/static/datatest2.xlsx" download><h4>Download Templet</h4></a></div>
                </div>
            </form>
            <br><br><br><br><br><br>
            <a href="#footer" class="goto-next scrolly">Next</a>
        </section>
        
        
        
        
        <section id="footer" class="wrapper style1 special fade-up">
            <!-- head -->
            <div class="content">
                <header>
                    <h2>Delete curriculum</h2>
                </header>
            </div>

            <!-- Delete -->
            <form method="post" action="#">
                <div class="row gtr-uniform gtr-50">
                        <!-- Faculty -->
                    <div class="col-3 col-12-medium"></div>
                    <div class="col-6 col-12-xsmall">
                        <select name="Select Faculty" id="defaculty" onchange="deshowfaculty()">
                            <option value="">Select Faculty</option>
                            <option value="human">01 Faculty of Humanity</option>
                            <option value="edu">02 Faculty of Education</option>
                            <option value="fineart">03 Faculty of Fine Art</option>
                            <option value="soc">04 Faculty of Social Sciences</option>
                            <option value="sci">05 Faculty of Science</option>
                            <option value="eng">06 Faculty of Engineering</option>
                            <option value="med">07 Faculty of Medicine</option>
                            <option value="agri">08 Faculty of Agriculture</option>
                            <option value="dent">09 Faculty of Dentistry</option>
                            <option value="phama">10 Faculty of Pharmacy</option>
                            <option value="medtec">11 Faculty of Associated Medical Sciences</option>
                            <option value="nurse">12 Faculty of Nursing</option>
                            <option value="argo">13 Faculty of Agro-Industry</option>
                            <option value="vet">14 Faculty of Veterinary Medicine</option>
                            <option value="accba">15 Faculty of Business Administration</option>
                            <option value="econ">16 Faculty of Economics</option>
                            <option value="arch">17 Faculty of Architecture</option>
                            <option value="masscom">18 Faculty of Mass Communication</option>
                            <option value="polsci">19 Faculty of Political Science and Public Administration</option>
                            <option value="law">20 Faculty of Law</option>
                            <option value="camt">21 Faculty of Collage of Art, Media and Technology</option>
                        </select>
                    </div>
                    <div class="col-3 col-12-medium"></div>

                    <!-- Bachelor -->
                    <div class="col-3 col-12-medium"></div>
                    <div class="col-6 col-12-xsmall">
                        <select name="Select Course" id="debachelor" onchange="deshowbachelor()">
                            <option value="">Select Bachelor</option>
                        </select>
                    </div>
                    <div class="col-3 col-12-medium"></div>

                    <!-- Search -->
                    <div class="col-4 col-12-medium"></div>
                    <div class="col-4 col-12-medium">
                        <input type="button" value="Search" class="button primary" id="recive" >
                    </div>
                    <div class="col-4 col-12-medium"></div>

                    <!-- course -->
                    <div class="col-3 col-12-medium"></div>
                    <div class="col-6 col-12-xsmall">
                        <select name="" id="course" ></select>
                    </div>
                    <div class="col-3 col-12-medium"></div>

                    <!-- Delete -->
                    <div class="col-4 col-12-medium" ></div>
                    <div class="col-4 col-12-medium" >
                        <input type="button" value="Delete" class="button primary" id="deleted">
                    </div>
                    <div class="col-4 col-12-medium"></div>
                </div>
            </form>
            <br><br><br><br><br><br>
        </section>
    </div>
    <div hidden>
        <textarea name="" id="keybin" cols="100" rows="1" ></textarea>
    </div>
     <!-- Scripts -->
     <script src="/static/css/jquery.min.js"></script>
     <script src="/static/css/jquery.scrolly.min.js"></script>
     <script src="/static/css/jquery.dropotron.min.js"></script>
     <script src="/static/css/jquery.scrollex.min.js"></script>
     <script src="/static/css/browser.min.js"></script>
     <script src="/static/css/breakpoints.min.js"></script>
     <script src="/static/css/util.js"></script>
     <script src="/static/css/main.js"></script>
     <script src = "/static/staff.js"></script>
     <script>

     </script>
</body>
</html>